{
    "editor.tabSize": 2,
    "editor.fontSize": 18,
    "files.autoSave": "afterDelay",
    "breadcrumbs.enabled": false,
    "better-comments.multilineComments": true,
    "better-comments.tags": [
        {
          "tag": "!",
          "color": "#FF2D00",
          "strikethrough": false,
          "backgroundColor": "transparent"
        },
        {
          "tag": "?",
          "color": "#3498DB",
          "strikethrough": false,
          "backgroundColor": "transparent"
        },
        {
          "tag": "//",
          "color": "#8d6e63",
          "strikethrough": true,
          "backgroundColor": "transparent"
        },
        {
          "tag": "todo",
          "color": "#FF8C00",
          "strikethrough": false,
          "backgroundColor": "transparent"
        },
        {
          "tag": "*",
          "color": "#98C379",
          "strikethrough": false,
          "backgroundColor": "transparent"
        }
      ],
      "workbench.colorCustomizations": {
        "editor.selectionBackground": "#0000ff",
        "window.activeBorder": "#9ccc65",
        "window.inactiveBorder": "#82b1ff",
        "activityBar.activeBorder": "#07000f",
        "[codeSTACKr Theme]": {
          "activityBar.activeBackground": "#87B6E6",
          "tab.activeBackground": "#0af820",
          "tab.inactiveBackground": "#898cf8",
          "tab.activeForeground": "#000",
          "tab.inactiveForeground": "#000",
          "editor.background": "#02042a"
        },
        "[Andromeda]": {
          "editor.background": "#02042a"
        },
        "[Coder Coder Dark Theme]": {
          "editor.background": "#02042a"
        },
        "activityBar.activeBackground": "#ff0000"
      }
}